        </annotation>
        <listOfReactants>
          <speciesReference species="dap_ext" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="dap_urine" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> f_renal_function </ci>
              <ci> DAPEX_k </ci>
              <ci> Vki </ci>
              <ci> dap_ext </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction metaid="meta_GLCEX" sboTerm="SBO:0000655" id="GLCEX" name="glucose excretion (GLCEX)" reversible="false" compartment="Vki">
        <annotation>
          <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:vCard="http://www.w3.org/2001/vcard-rdf/3.0#" xmlns:vCard4="http://www.w3.org/2006/vcard/ns#" xmlns:bqbiol="http://biomodels.net/biology-qualifiers/" xmlns:bqmodel="http://biomodels.net/model-qualifiers/">
            <rdf:Description rdf:about="#meta_GLCEX">
              <bqbiol:is>
                <rdf:Bag>
                  <rdf:li rdf:resource="http://identifiers.org/SBO:0000655"/>
                </rdf:Bag>
              </bqbiol:is>
            </rdf:Description>
          </rdf:RDF>
        </annotation>
        <listOfReactants>
          <speciesReference species="glc_ext" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="glc_urine" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <listOfModifiers>
          <modifierSpeciesReference species="dap_ext"/>
        </listOfModifiers>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML" xmlns:sbml="http://www.sbml.org/sbml/level3/version2/core">
            <piecewise>
              <piece>
                <apply>
                  <times/>
                  <apply>
                    <divide/>
                    <ci> GFR </ci>
                    <ci> cf_ml_per_l </ci>
                  </apply>
                  <apply>
                    <minus/>
