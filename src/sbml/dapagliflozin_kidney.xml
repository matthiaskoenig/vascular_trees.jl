                  </apply>
                </apply>
                <apply>
                  <gt/>
                  <ci> glc_ext </ci>
                  <ci> RTG </ci>
                </apply>
              </piece>
              <otherwise>
                <cn sbml:units="mmole_per_min" type="integer"> 0 </cn>
              </otherwise>
            </piecewise>
          </math>
        </kineticLaw>
      </reaction>
